{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2020",
    "outDir": "lib",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "declaration": false,
    "sourceMap": false,
    "noUnusedLocals": true,
    "esModuleInterop": true
  },
  "exclude": ["template", "lib"]
}
